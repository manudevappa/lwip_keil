<?xml version="1.0" encoding="UTF-8"?>
<projectSpec>
    <applicability>
        <when>
            <context
                deviceFamily="ARM"
                deviceId="Cortex R.AM64x"
            />
        </when>
    </applicability>

    <project
        title="Ethernetip Adapter Generic Device Mii Demo"
        name = "ethernetip_adapter_generic_device_mii_demo_am64x-evm_r5fss0-0_freertos_ti-arm-clang"
