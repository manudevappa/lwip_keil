            -mcpu=cortex-r5
            -mfloat-abi=hard
            -mfpu=vfpv3-d16
            -mthumb
            -Wall
            -Werror
            -gdwarf-3
            -Wno-gnu-variable-sized-type-not-at-end
            -Wno-unused-function
            -Wno-unused-but-set-variable
            -mllvm
            -align-all-functions=2
            -DSOC_AM64X
            -DOS_FREERTOS
            -DOSAL_FREERTOS=1
            -DSOC_AM64X
            -DSOC_AM64X=1
            -DCUST_PHY_DP83869=1
            -DEIP_TIME_SYNC=1
            -DEIP_QUICK_CONNECT=0
            -DCPU_LOAD_MONITOR=0
        "
        linkerBuildOptions="
            -i${MCU_PLUS_SDK_PATH}/source/kernel/freertos/lib
            -i${MCU_PLUS_SDK_PATH}/source/drivers/lib
            -i${MCU_PLUS_SDK_PATH}/source/board/lib
            -i${MCU_PLUS_SDK_PATH}/source/networking/icss_emac/lib
